{
  "log": {
      "loglevel": "debug"
  },
  "inbounds": [
      {
          "port": ${PORT},
          "protocol": "vmess",
          "settings": {
              "clients": [
                  {
                      "id": "${UUID}"
                  }
              ]
          },
          "streamSettings": {
              "network": "ws",
              "wsSettings": {
                "path": "${PATH}"
              },
              "security": "tls",
              "tlsSettings": {
                "allowInsecure": false
              }
          }
      }
  ],
  "outbounds": [
      {
          "protocol": "freedom",
          "settings": {}
      }
  ]
}