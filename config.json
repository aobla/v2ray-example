{
    "log": {
      "loglevel": "warning"
    },
    "routing": {
      "domainStrategy": "AsIs",
      "rules": [
          {
              "type": "field",
              "ip": [
                  "geoip:private"
              ],
              "outboundTag": "block"
          }
      ]
    },
    "inbounds": [
      {
        "port": "${PORT}",
        "protocol": "vmess",
        "settings": {
          "clients": [
            {
              "id": "${UUID}",
              "level": 0
            }
          ],
          "decryption": "none"
        },
        "streamSettings": {
          "network": "ws",
          "wsSettings": {
            "path": "/v2ray"
          }
        }
      }
    ],
    "outbounds": [
      {
        "protocol": "freedom",
        "tag": "direct"
      },
      {
        "protocol": "blackhole",
        "tag": "block"
      }
    ]
  }