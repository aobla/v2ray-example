{
  "log": {
      "loglevel": "debug"
  },
  "routing": {
      "domainStrategy": "AsIs",
      "rules": [
          {
              "type": "field",
              "ip": [
                  "geoip:private"
              ],
              "outboundTag": "block"
          }
      ]
  },
  "inbounds": [
      {
          "listen": "0.0.0.0",
          "port": "${PORT}",
          "protocol": "vmess",
          "settings": {
              "clients": [
                  {
                      "id": "${UUID}"
                  }
              ]
          },
          "streamSettings": {
              "network": "ws",
              "wsSettings": {
                "path": "/vmess"
              }
              "security": "tls",
              "tlsSettings": {
                "allowInsecure": false
              }
          }
      }
  ],
  "outbounds": [
      {
          "protocol": "freedom",
          "tag": "direct"
      },
      {
          "protocol": "blackhole",
          "tag": "block"
      }
  ]
}