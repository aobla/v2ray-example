{
    "log": {
      "loglevel": "warning"
    },
    "inbounds": [
      {
        "port": "${PORT}",
        "protocol": "vmess",
        "settings": {
          "clients": [
            {
              "id": "${UUID}",
              "level": 0
            }
          ],
          "decryption": "none"
        },
        "streamSettings": {
          "network": "ws",
          "wsSettings": {
            "path": "/v2ray"
          }
        }
      }
    ],
    "outbounds": [
      {
        "protocol": "freedom",
        "tag": "direct"
      }
    ]
  }